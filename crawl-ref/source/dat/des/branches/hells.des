function dis_setup(e)
   e.place("Dis:7")
   e.tags("dis", "no_rotate")
   e.lflags("no_tele_control")
   e.lua_marker('O', item_pickup_change_flags {
                     level_flags="!no_tele_control", item="auto" } )
   e.kmask('O = no_item_gen')
   e.kitem('O = iron rune of Zot')
