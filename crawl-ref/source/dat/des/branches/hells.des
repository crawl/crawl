                     level_flags="!no_tele_control", item="auto" } )
   e.kmask('O = no_item_gen')
   e.kitem('O = obsidian rune of Zot')
   e.lrockcol("brown")
   e.lfloorcol("darkgrey")
--   e.lrocktile("???")
--   e.lfloortile("???")
   e.colour("v = red")
